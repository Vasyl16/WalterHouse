<section class='swiper-main__list-services list-services'>
  <picture
    style="
        background-image: url('/img/list-services/list-services-bg-mini.jpg');
      "
    class='list-services__image-bg-wrapper lazy-loading-img-wrapper'
  >
    <source
      type='image/webp'
      srcset='
          /img/list-services/list-services-bg.webp    1x,
          /img/list-services/list-services-bg@2x.webp 2x
        '
      media='(min-width: 1025px)'
    />

    <source
      type='image/webp'
      srcset='
          /img/list-services/list-services-bg-medium.webp    1x,
          /img/list-services/list-services-bg-medium@2x.webp 2x
        '
      media='(min-width: 451px) and (max-width: 1024px)'
    />

    <source
      type='image/webp'
      srcset='
          /img/list-services/list-services-bg-small.webp    1x,
          /img/list-services/list-services-bg-small@2x.webp 2x
        '
      media='(max-width: 450px)'
    />

    <source
      type='image/jpeg'
      srcset='
          /img/list-services/list-services-bg-medium.jpg    1x,
          /img/list-services/list-services-bg-medium@2x.jpg 2x
        '
      media='(min-width: 451px) and (max-width: 1024px)'
    />

    <source
      type='image/jpeg'
      srcset='
          /img/list-services/list-services-bg-small.jpg    1x,
          /img/list-services/list-services-bg-small@2x.jpg 2x
        '
      media='(max-width: 450px)'
    />

    <img
      loading='lazy'
      class='renovation__image-bg lazy-loading-img'
      srcset='
          /img/list-services/list-services-bg.jpg    1x,
          /img/list-services/list-services-bg@2x.jpg 2x
        '
      alt='img'
    />
  </picture>

  <div class='list-services__container'>
    <div class='list-services__body'>
      <h1 class='list-services__title'>
        <span> </span>
        Select an area size &

        <span> one of the service packages: </span>
      </h1>

      <div class='list-services__items'>
        <div class='list-services__item'>
          <div class='list-services__circle-size-wrapper'>
            <p class='list-services__circle-size-unit'>
              m
              <span>2</span>
            </p>

            <svg class='list-services__progress-circle-wrapper'>
              <circle
                cx='50%'
                cy='50%'
                r='49%'
                class='list-services__progress-circle-line'
              ></circle>

              <circle
                cx='50%'
                cy='50%'
                r='49%'
                class='list-services__progress-circle'
              ></circle>
            </svg>

            <div class='list-services__progress-point-handle'></div>

            <button class='list-services__progress-point'></button>

            <p class='list-services__circle-size-count'>64</p>
          </div>
        </div>

        <div class='list-services__item'>
          <div class='list-services__package-size package-size-list-services'>
            <div
              class='package-size-list-services__item package-size-list-services__item_elite'
            >
              <div
                class='package-size-list-services__info-icon package-size-list-services__info-icon_elite'
              >
                i
              </div>

              <div
                class='package-size-list-services__package-wrapper package-size-list-services__package-wrapper_active'
                data-value='1000'
              >
                <p
                  class='package-size-list-services__type package-size-list-services__type_first'
                >
                  Elite
                </p>

                <div class='package-size-list-services__lines'>
                  <div class='package-size-list-services__first-line'></div>

                  <div class='package-size-list-services__circle'></div>

                  <div
                    class='package-size-list-services__second-line package-size-list-services__second-line_elite'
                  ></div>
                </div>
              </div>
            </div>

            <div
              class='package-size-list-services__item package-size-list-services__item_vip'
            >
              <div
                class='package-size-list-services__info-icon package-size-list-services__info-icon_vip'
              >
                i
              </div>

              <div
                class='package-size-list-services__package-wrapper'
                data-value='1500'
              >
                <p
                  class='package-size-list-services__type package-size-list-services__type_center'
                >
                  vip
                </p>

                <div class='package-size-list-services__lines'>
                  <div class='package-size-list-services__second-line'></div>

                  <div class='package-size-list-services__circle'></div>

                  <div class='package-size-list-services__second-line'></div>
                </div>
              </div>
            </div>

            <div
              class='package-size-list-services__item package-size-list-services__item_extra'
            >
              <div
                class='package-size-list-services__info-icon package-size-list-services__info-icon_extra'
              >
                i
              </div>

              <div
                class='package-size-list-services__package-wrapper'
                data-value='2000'
              >
                <p
                  class='package-size-list-services__type package-size-list-services__type_last'
                >
                  Extra
                </p>

                <div class='package-size-list-services__lines'>
                  <div
                    class='package-size-list-services__second-line package-size-list-services__second-line_extra'
                  ></div>

                  <div class='package-size-list-services__circle'></div>

                  <div
                    class='package-size-list-services__second-line_last'
                  ></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class='list-services__item list-services__item_last'>
          <p class='list-services__cost'>
            <span class='list-services__cost-count'>233 000</span>
            <span class='list-services__cost-unit'>uah</span>
          </p>
        </div>
      </div>
    </div>
  </div>
</section>

